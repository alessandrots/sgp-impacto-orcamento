{
  "name": "Impacto_Orcamentario",
  "description": "Modulo cliente do angular2.",
  "homepage": "www.mpf.mp.br",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "clean:typings": "rimraf node_modules/angular2/manual_typings",
    "postinstall": "npm run clean:typings && typings install",
    "prebuild": "npm run clean",
    "build": "webpack --config webpack.prod.config.js --progress --profile --colors --display-error-details --display-cached",
    "start": "webpack-dev-server --inline --colors --progress --display-error-details --disaply-modules --display-cached --port 8080",
    "test": "karma start karma.conf.js",
    "predeploy": "npm run build && rimraf ../server/build/public && mkdirp ../server/build/public",
    "deploy": "copyup dist/* ../server/build/public"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "bootstrap": "^3.3.6",
    "jquery": "^2.2.2",
    "ng2-pagination": "0.3.2",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.26",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "angular-cli": "^1.0.0-beta.5",
    "codelyzer": "0.0.19",
    "compression-webpack-plugin": "^0.2.0",
    "copy-webpack-plugin": "^1.1.1",
    "copyfiles": "^0.2.1",
    "css-loader": "^0.23.1",
    "jasmine-core": "^2.4.1",   
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.8",
    "karma-mocha-reporter": "^2.0.0",
    "karma-webpack": "^1.7.0",
    "file-loader": "^0.8.5",
    "mkdirp": "^0.5.1",
    "node-static": "^0.7.7",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.3",
    "ts-loader": "^0.8.1",
    "typescript": "^1.8.10",
    "typings": "^0.8.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
